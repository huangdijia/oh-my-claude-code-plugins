{
  "name": "git-flow",
  "version": "1.0.0",
  "description": "Comprehensive Git Flow automation with hooks and commands",
  "author": "Deeka Wong",
  "license": "MIT",
  "homepage": "https://github.com/huangdijia/oh-my-claude-code-plugins/tree/main/plugins/git-flow",
